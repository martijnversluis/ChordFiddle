{
  "name": "ChordFiddle",
  "version": "1.5.2",
  "description": "ChordPro editor. Transpose your chord sheet. Convert ChordPro to chord sheet.",
  "main": "index.html",
  "scripts": {
    "start": "REACT_APP_DESCRIPTION=$(node -p 'require(\"./package.json\").description') REACT_APP_NAME=$(node -p 'require(\"./package.json\").name') REACT_APP_KEYWORDS=$(node -p 'require(\"./package.json\").keywords.join(\", \")') react-scripts-x start",
    "build": "rm -rf static/ && REACT_APP_DESCRIPTION=$(node -p 'require(\"./package.json\").description') REACT_APP_NAME=$(node -p 'require(\"./package.json\").name') REACT_APP_KEYWORDS=$(node -p 'require(\"./package.json\").keywords.join(\", \")') react-scripts-x build && mv ./build/* ./ && rm -rf build/ service-worker.js",
    "test": "react-scripts-x test --env=jsdom",
    "eject": "react-scripts-x eject",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/martijnversluis/chordfiddle.git"
  },
  "keywords": [
    "javascript",
    "chords",
    "parsing",
    "convert",
    "chord-sheet",
    "chordpro"
  ],
  "author": "martijnversluis@gmail.com",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/martijnversluis/chordfiddle/issues"
  },
  "homepage": ".",
  "githubHome": "https://github.com/martijnversluis/ChordFiddle",
  "about": "https://github.com/martijnversluis/ChordFiddle/blob/master/README.md#chordfiddle",
  "dependencies": {
    "chordjs": "^1.2.0",
    "chordsheetjs": "^2.4.4",
    "lz-string": "^1.4.4",
    "postcss-calc": "^7.0.4",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "react-scripts": "1.0.17",
    "react-scripts-x": "^0.1.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.14.0",
    "expect": "^24.9.0",
    "jest-plugin-context": "^2.9.0",
    "redux-mock-store": "^1.5.4"
  },
  "react-scripts-x": {
    "postcss": [
      {
        "name": "postcss-import"
      },
      {
        "name": "postcss-custom-properties",
        "config": {
          "preserve": true
        }
      },
      {
        "name": "postcss-calc"
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.test.{js,jsx}",
      "!src/test/**"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ]
  }
}
